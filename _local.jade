#local
	#map_canvas

	script.
		function initialize() {
			var myLatlng = new google.maps.LatLng(-22.9617985,-43.2152676);
			var mapOptions = {
				scrollwheel: false,
				center: new google.maps.LatLng(-22.9617985,-43.2152676),
				zoom: 15,
				mapTypeId: google.maps.MapTypeId.ROADMAP
			};
			var map = new google.maps.Map(document.getElementById("map_canvas"), mapOptions);
			var marker = new google.maps.Marker({
				position: myLatlng,
				map: map,
				title: 'Huge Rio'
			});
		}
		initialize()